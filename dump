SELECT
    S.SCENARIO_ID
FROM
    RT_SCENARIOANALYSIS AS S
LEFT JOIN BO_ENUMVALS AS E_CHALLENGEDYN ON E_CHALLENGEDYN.ENUMVALID = S.CHALLENGEDYN
LEFT JOIN BO_ENUMVALS AS E_DELMARK ON E_DELMARK.ENUMVALID = S.DELMARK
LEFT JOIN BO_ENUMVALS AS E_STATUS ON E_STATUS.ENUMVALID = S.STATUS00
LEFT JOIN BO_ENUMVALS AS E_DIVISION ON E_DIVISION.ENUMVALID = S.DIVISION
LEFT JOIN BO_ENUMVALS AS E_RISKTYPEL1 ON E_RISKTYPEL1.ENUMVALID = S.RISKTYPEL1
LEFT JOIN BO_ENUMVALS AS E_RISKTYPEL2 ON E_RISKTYPEL2.ENUMVALID = S.RISKTYPEL2
LEFT JOIN BO_ENUMVALS AS E_RISKTYPEL3 ON E_RISKTYPEL3.ENUMVALID = S.RISKTYPEL3
LEFT JOIN BO_ENUMVALS AS E_PREQ ON E_PREQ.ENUMVALID = S.PREQ
LEFT JOIN ACTORINFO AS U_CHECKED_IN_BY ON U_CHECKED_IN_BY.ACTORID = S.CHECKED_IN_BY
LEFT JOIN ACTORINFO AS U_CREATOR ON U_CREATOR.ACTORID = S.CREATOR
LEFT JOIN BO_ENUMVALS AS E_UWRITEPERCENT ON E_UWRITEPERCENT.ENUMVALID = S.UWRITEPERCENT
LEFT JOIN BO_ENUMVALS AS E_PROFITPERCENT ON E_PROFITPERCENT.ENUMVALID = S.PROFITPERCENT
LEFT JOIN BO_ENUMVALS AS E_BROKERPERCENT ON E_BROKERPERCENT.ENUMVALID = S.BROKERPERCENT
LEFT JOIN BO_ENUMVALS AS E_NONPROFITPERCENT ON E_NONPROFITPERCENT.ENUMVALID = S.NONPROFITPERCENT
LEFT JOIN BO_ENUMVALS AS E_ACTIONRQD ON E_ACTIONRQD.ENUMVALID = S.ACTIONRQD
LEFT JOIN BO_ENUMVALS AS E_DIROPLOSS_BC ON E_DIROPLOSS_BC.ENUMVALID = S.DIROPLOSS_BC
LEFT JOIN BO_ENUMVALS AS E_DIROPLOSS_LC ON E_DIROPLOSS_LC.ENUMVALID = S.DIROPLOSS_LC
LEFT JOIN BO_ENUMVALS AS E_INDIROPLOSS_BC ON E_INDIROPLOSS_BC.ENUMVALID = S.INDIROPLOSS_BC
LEFT JOIN BO_ENUMVALS AS E_INDIROPLOSS_LC ON E_INDIROPLOSS_LC.ENUMVALID = S.INDIROPLOSS_LC
LEFT JOIN BO_ENUMVALS AS E_OTHERFINLOSS_BC ON E_OTHERFINLOSS_BC.ENUMVALID = S.OTHERFINLOSS_BC
LEFT JOIN BO_ENUMVALS AS E_OTHERFINLOSS_LC ON E_OTHERFINLOSS_LC.ENUMVALID = S.OTHERFINLOSS_LC
LEFT JOIN BO_ENUMVALS AS E_TOTFINIMPACT_BC ON E_TOTFINIMPACT_BC.ENUMVALID = S.TOTFINIMPACT_BC
LEFT JOIN BO_ENUMVALS AS E_TOTFINIMPACT_LC ON E_TOTFINIMPACT_LC.ENUMVALID = S.TOTFINIMPACT_LC
LEFT JOIN BO_ENUMVALS AS E_VERSION ON E_VERSION.ENUMVALID = S.VERSION00
LEFT JOIN BO_ENUMVALS AS E_ORGANISATION ON E_ORGANISATION.ENUMVALID = S.ORGANISATION
LEFT JOIN BO_ENUMVALS AS E_CATL1 ON E_CATL1.ENUMVALID = S.CATL1
LEFT JOIN BO_ENUMVALS AS E_CATL2 ON E_CATL2.ENUMVALID = S.CATL2
WHERE
    S.REPORTING_PERIOD_ID = -1
